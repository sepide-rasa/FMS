@model IEnumerable<NewFMS.WCF_Common.OBJ_FormatFile>
@using Ext.Net.MVC
@using Hogaf.ExtNet.UX
@using Ext.Net;

@{ var X = Html.X();}

<script>
    var ShowIconFormat = function (value) {
        if (value == "") {
            return "";
        }
        else {
            pic = "data:image/png;base64," + value;
            var template = "<img src=" + pic + " style='width:17px;height:17px;' />";
            return template;
        }
    };
</script>

@(Html.X().Panel()
    .ID("FormatFiles")
    .Title("ثبت فرمت های مجاز")
    .Closable(true)
    .Listeners(li => li.Render.Fn = "addKeyMapFormatFiles")
    .CloseAction(Ext.Net.CloseAction.Destroy)
    .Border(false)
    .Items(
        X.Panel()
            .Border(false)
            .Buttons(
                X.Button().Text("ذخیره").Icon(Ext.Net.Icon.Disk).Listeners(l => l.Click.Handler = "SaveFormatFiles();").ItemID("btnSave"),
                X.Button().Text("ویرایش").Icon(Ext.Net.Icon.Pencil).Listeners(l => l.Click.Handler = "LoadFormatFiles();"),
                X.Button().Text("حذف").Icon(Ext.Net.Icon.Delete).Listeners(l => l.Click.Handler = "DeleteFormatFiles();"),
                X.Button().Text("راهنما").Icon(Ext.Net.Icon.Help).Listeners(l => l.Click.Handler = "HelpFormatFiles();"),
                X.Button().Text("خروج").Icon(Ext.Net.Icon.DoorOut).OnClientClick("App.FormatFiles.destroy()")
                )
            .ButtonAlign(Ext.Net.Alignment.Center)
            .Items(
                X.Panel()
                    .Border(false)
                    .Layout(LayoutType.VBox)
                    .LayoutConfig(new VBoxLayoutConfig { Align = VBoxAlign.Center })
                    .Items(
                        X.FieldSet()
                            .Title("فرمت فایل")
                            .Height(120)
                            .Border(true)
                            .Defaults(new { Margin = 2 })
                            .Layout(LayoutType.Table)
                            .LayoutConfig(new TableLayoutConfig { Columns = 6 })
                            .Items(
                                X.Hidden()
                                    .ItemID("fldId"),
                                X.Label("نام فرمت:"),
                                X.TextField()
                                    .ItemID("txtTitle")
                                    .AllowBlank(false)
                                    .BlankText("لطفا نام فرمت را وارد کنید.")
                                    .MaxLength(50)
                                    .EnforceMaxLength(true)
                                    .Listeners(l => l.AfterRender.Handler = "App.FormatFiles.queryById('txtTitle').validate()")                                    
                                    .AutoFocus(true),
                                X.Label("پسوندهای مجاز:"),
                                X.TextField()
                                    .ItemID("txtPassvand")
                                    .Listeners(l => l.AfterRender.Handler = "App.FormatFiles.queryById('txtPassvand').validate()") 
                                    .AllowBlank(false)
                                    .BlankText("لطفا پسوندهای مجاز را وارد کنید.")
                                    .MaxLength(100)
                                    .EnforceMaxLength(true)
                                    .EmptyText("مثال: jpg, png, jpeg")
                                    .MaskRe("[a-z,,]"),
                                 X.Label("سایز فایل (MB):"),
                                 X.TextField()
                                    .ItemID("txtSize")
                                    .AllowBlank(false)
                                    .BlankText("لطفا سایز فایل را وارد کنید."),
                                X.Label("توضیحات:"),
                                X.TextField()
                                    .ItemID("txtDesc")
                                    .Width(382)
                                    .ColSpan(6),
                                X.FormPanel()
                                    .Border(false)
                                    .ColSpan(6)
                                    .Layout(LayoutType.Table)
                                    .LayoutConfig(new TableLayoutConfig{Columns=2})
                                    .Items(
                                        X.FileUploadField()
                                            .MarginSpec("10px 10px 10px 0")
                                            .Icon(Ext.Net.Icon.DiskUpload)
                                            .ButtonOnly(true)
                                            .ButtonText("انتخاب فایل")
                                            .DirectEvents(l =>
                                            {
                                                l.Change.Action = "Upload";
                                                l.Change.Success = "hideprogressFormat();";
                                            })
                                            .Listeners(l => l.Change.Fn = "showFileFormat"),
                                        X.Container()
                                            .Content(@<div id="fi-button-msgFormat" style="width:340px;text-align:right;"><b>فایل انتخاب شده:</b></div>)
                                        )
                        )
                    )
        ),
        X.GridPanel()
            .ItemID("FormatFilesGrid")
            .BottomBar(X.PagingToolbar())
            .Store(
                X.StoreForModel()
                    .Listeners(l => l.DataChanged.Handler = "App.FormatFiles.queryById('FormatFilesGrid').selModel.refresh();")
                    .Proxy(Html.X().AjaxProxy()
                        .Url(Url.Action("Read"))
                        .Reader(Html.X().JsonReader().Root("data"))
                    )
                    .PageSize(20)
                    .RemoteFilter(true)
                    .RemotePaging(true)
                )
            .View(
                Html.X().GridView().LoadingText("در حال بارگذاری...").RTL(true)
            )
            .Plugins(X.FilterHeader().Remote(true))
                .Listeners(l => l.AfterRender.Handler = "App.FormatFiles.queryById('FormatFilesGrid').setHeight(App.Win.getHeight() - 330); ")
            .ColumnModel(
                Html.X().RowNumbererColumn(),
                Html.X().Column().DataIndex(Model, m => m.fldId).Text("کد").Hidden(true).Flex(1),
                Html.X().Column().DataIndex(Model, m => m.fldIcon).Text("").Filterable(false).Renderer("ShowIconFormat").Flex(1),
                Html.X().Column().DataIndex(Model, m => m.fldFormatName).Text("عنوان").Wrap(true).Flex(4),
                Html.X().Column().DataIndex(Model, m => m.fldPassvand).Text("پسوندهای مجاز").Wrap(true).Flex(4),
                Html.X().Column().DataIndex(Model, m => m.fldDesc).Text("توضیحات").Flex(10)
            )
            .SelectionModel(
                Html.X().RowSelectionModel()
                    .Mode(Ext.Net.SelectionMode.Single)
        )
    )
)
<script type="text/javascript">
    var MojazFormat = 0;
    var sizeeFormat = 0;
    var EndUploadFormat = false;

    var CheckFormat = setInterval(CheckEndFormat, 500);

    function CheckEndFormat() {
        if (EndUploadFormat == true) {
            Ext.MessageBox.hide();
            Ext.Msg.alert('عملیات موفق', 'فایل با موفقیت آپلود شد.');
            clearInterval(CheckEndFormat);
            EndUploadFormat = false;
        }
    }

    var showFileFormat = function (fb, v) {
        var id = fb.fileInputEl.dom.id;
        var el = Ext.get('fi-button-msgFormat');
        if (v) {
            showProgressBarFormat();
            if (v.split('.').pop().toLowerCase() == "jpg" || v.split('.').pop().toLowerCase() == "png" || v.split('.').pop().toLowerCase() == "jpeg" || v.split('.').pop().toLowerCase() == "tif" || v.split('.').pop().toLowerCase() == "tiff") {
                MojazFormat = 1;
                if (document.getElementById(id).files[0].size <= 1024000) {
                    sizeeFormat = 1;
                }
                if (sizeeFormat == 1) {
                    el.update('<b>فایل انتخاب شده:</b> ' + v);
                }
                else {
                    el.update('<b>فایل انتخاب شده:</b> ');
                }

                if (!el.isVisible()) {
                    el.slideIn('t', {
                        duration: .2,
                        easing: 'easeIn',
                        callback: function () {
                            el.highlight();
                        }
                    });
                } else {
                    el.highlight();
                }
            }
            else {
                el.update('<b>فایل انتخاب شده:</b> ');
            }
        }
    };

    function hideprogressFormat() {
        if (MojazFormat == 1 && sizeeFormat == 1) {
            EndUploadFormat = true;
        }
        MojazFormat = 0;
        sizeeFormat = 0;
    }

    function showProgressBarFormat() {
        Ext.MessageBox.show({
            msg: 'لطفاً منتظر باشید',
            progressText: 'در حال آپلود فایل...',
            width: 300,
            wait: true,
            waitConfig:
            {
                interval: 200,
                text: 'در حال آپلود فایل...',
                scope: this
            }
        });
    }

    function LoadFormatFiles() {
        var SelectedRow = App.FormatFiles.queryById('FormatFilesGrid').selModel.getLastSelected();
        if (SelectedRow != null) {
            Ext.net.Mask.show({ msg: 'در حال بارگذاری...' });
            Ext.net.DirectMethod.request({
                url: '/comon/Format/Details',
                params: {
                    Id: SelectedRow.data.fldId
                },
                success: function (data) {
                    
                    var txtTitle = App.FormatFiles.queryById('txtTitle');
                    txtTitle.setValue(data.fldTitle);
                    var txtSize = App.FormatFiles.queryById('txtSize');
                    txtSize.setValue(data.fldSize);
                    var txtPassvand = App.FormatFiles.queryById('txtPassvand');
                    txtPassvand.setValue(data.fldPassvand);
                    var txtDesc = App.FormatFiles.queryById('txtDesc');
                    txtDesc.setValue(data.fldDesc);
                    var fldId = App.FormatFiles.queryById('fldId');
                    fldId.setValue(data.fldId);
                    Ext.net.Mask.hide();
                }
            });
        }
        else {
            Ext.MessageBox.show({
                title: "خطا",
                msg: "لطفا یک سطر را انتخاب کنید.",
                icon: Ext.MessageBox.ERROR,
                buttons: Ext.MessageBox.OK
            });
        }
    }

    function SaveFormatFiles() {
        var txtTitle = App.FormatFiles.queryById('txtTitle');
        var txtPassvand = App.FormatFiles.queryById('txtPassvand');
        var txtDesc = App.FormatFiles.queryById('txtDesc');
        var fldId = App.FormatFiles.queryById('fldId');
        var txtSize = App.FormatFiles.queryById('txtSize');
        var er = false;

        if (txtTitle.validate() == false) {
            er = true;
        }
        if (txtPassvand.validate() == false) {
            er = true;
        }
        if (txtSize.validate() == false) {
            er = true;
        }
        if (er == true)
            return;

        Ext.net.Mask.show({ msg: 'در حال بارگذاری...' });
        Ext.net.DirectMethod.request({
            url: '/comon/Format/Save',
            params: {
                fldId: fldId.getValue(),
                fldFormatName: txtTitle.getValue(),
                fldPassvand: txtPassvand.getValue(),
                fldDesc: txtDesc.getValue(),
                fldSize: txtSize.getValue()
            },
            success: function (data) {
                var ic = Ext.MessageBox.INFO;
                if (data.Er == 1)
                    ic = Ext.MessageBox.ERROR;
                Ext.MessageBox.show({
                    title: data.MsgTitle,
                    msg: data.Msg,
                    icon: ic,
                    buttons: Ext.MessageBox.OK
                });
                Ext.net.Mask.hide();
                App.FormatFiles.queryById('FormatFilesGrid').store.load();
                if (data.Er != 1)
                    ClearFormat();
            }

        });
    }

    function DeleteFormatFiles() {
        var SelectedRow = App.FormatFiles.queryById('FormatFilesGrid').selModel.getLastSelected();
        if (SelectedRow != null) {
            Ext.MessageBox.show({
                title: "هشدار",
                msg: "آیا برای حذف مطمئن هستید؟",
                icon: Ext.MessageBox.WARNING,
                buttons: Ext.MessageBox.YESNO,
                fn: function (btn) {
                    if (btn == 'yes') {
                        Ext.net.Mask.show({ msg: 'در حال بارگذاری...' });
                        Ext.net.DirectMethod.request({
                            url: '/comon/Format/Delete',
                            params: {
                                id: SelectedRow.data.fldId
                            },
                            success: function (data) {
                                var ic = Ext.MessageBox.INFO;
                                if (data.Er == 1)
                                    ic = Ext.MessageBox.ERROR;
                                Ext.MessageBox.show({
                                    title: data.MsgTitle,
                                    msg: data.Msg,
                                    icon: ic,
                                    buttons: Ext.MessageBox.OK
                                });
                                App.FormatFiles.queryById('FormatFilesGrid').store.load()
                            }
                        });
                        Ext.net.Mask.hide();
                    }
                }
            });
        }
        else {
            Ext.MessageBox.show({
                title: "خطا",
                msg: "لطفا یک سطر را انتخاب کنید.",
                icon: Ext.MessageBox.ERROR,
                buttons: Ext.MessageBox.OK
            });
        }
    }

    function ClearFormat() {
        App.FormatFiles.queryById('fldId').setValue(0);
        App.FormatFiles.queryById('txtTitle').setValue("");
        App.FormatFiles.queryById('txtPassvand').setValue("");
        App.FormatFiles.queryById('txtDesc').setValue("");
        App.FormatFiles.queryById('txtSize').setValue("");
        Ext.get('fi-button-msgFormat').update("");
    }

    function HelpFormatFiles() {
        /*Ext.net.Mask.show({ msg: 'در حال بارگذاری...' });
        Ext.net.DirectMethod.request({
            url: '/Archive/Format/Help',
            success: function () {
                Ext.net.Mask.hide();
            }
        });*/
    }

    var addKeyMapFormatFiles = function (cmp) {
        this.keyMap = new Ext.util.KeyMap({
            target: cmp.getEl(),
            binding: [{
                key: [13, 9], /*Ext.EventObject.ENTER,*/
                fn: function (key, e) {
                    if (e.getKey() == 9) {
                        e.stopEvent();
                    }
                    if (Ext.Element.getActiveElement().name == "") {
                        SaveFormatFiles();
                        return;
                    }
                    var keyfield = App.FormatFiles.queryById(Ext.getCmp((Ext.Element.getActiveElement()).name).itemId);
                    var nextfld;
                    if (keyfield.itemId == "txtDesc") {
                        nextfld = App.FormatFiles.queryById("btnSave");
                        nextfld.focus();
                        return;
                    }
                    else {
                        nextfld = keyfield.nextSibling().nextSibling();
                    }
                    nextfld.focus();
                    nextfld.selectText();
                }
            }]
        });
    }
</script>