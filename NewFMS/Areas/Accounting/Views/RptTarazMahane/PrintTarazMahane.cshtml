@using Ext.Net.MVC;
@(
 Html.X().Window().Height(560).Width(1200)
            .Title("پیش نمایش")
            .AutoScroll(false)
            .Closable(true)
                .Modal(true)
            .CloseAction(Ext.Net.CloseAction.Destroy)
            .Html("<center><object class='se-pre-con' style='width: 1200px;height: 550px;border: 1px solid #ccc;' type='application/pdf' data='/Accounting/RptTarazMahane/PdfTarazMahane?Year=" + Convert.ToInt16(@ViewBag.salMali) + "&AzTarikh=" + @ViewBag.AzTarikh 
               +"&TaTarikh=" + @ViewBag.TaTarikh + "&AzLevel=" + @ViewBag.AzLevel + "&TaLevel=" + @ViewBag.TaLevel + "&AzSanad=" + @ViewBag.AzSanad + "&TaSanad=" + @ViewBag.TaSanad + "&StartNodeID=" + Convert.ToInt32(@ViewBag.StartNodeID) + "&sanadtype=" + @ViewBag.sanadtype +"&Taraz="+@ViewBag.Taraz+ "'></object></center>")

)

