                cellediting.getEditor(opt.record, opt.column).field.setValue(opt.record.data.fldName_CodeDetail);
                cellediting.getEditor(opt.record, opt.column).field.getStore().getProxy().setExtraParam("Year", '@ViewBag.Year');
