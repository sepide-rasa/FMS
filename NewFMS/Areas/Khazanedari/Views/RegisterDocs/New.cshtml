    function LoadDocRegister_DetailsKhazane() {
        if ('@ViewBag.Required' == 1) {
            App.DocRegister_DetailsKhazane.queryById("txtArchiveNum").allowBlank = false;
        }
        else {
            App.DocRegister_DetailsKhazane.queryById("txtArchiveNum").allowBlank = true;
        }
        if ('@ViewBag.HeaderId' != "0") {
            Ext.net.Mask.show({ msg: 'در حال بارگزاری...' });
            Ext.net.DirectMethod.request({
                url: '/Accounting/DocRegistration/Details',
                params: {
                    HeaderId: '@ViewBag.HeaderId',
                    Year: '@ViewBag.Year',
                    ModuleId: 10
                },
                success: function (data) {
                    App.DocRegister_DetailsKhazane.queryById("fldHeaderId").setValue(data.fldId);
                    App.DocRegister_DetailsKhazane.queryById("fldType").setValue(data.fldType);
                    if (data.fldType != 2) {/*یادداشت نبود*/
                        App.DocRegister_DetailsKhazane.queryById("SavedDocNum").setValue(data.fldDocumentNum);
                        App.DocRegister_DetailsKhazane.queryById("txtDocNum").setValue(data.fldDocumentNum);
                        App.DocRegister_DetailsKhazane.queryById("txtAtfNum").setValue(data.fldAtfNum);
                        App.DocRegister_DetailsKhazane.queryById("txtDaily").setValue(data.ShomareRoozane);
                    }
                    else {
                        Ext.net.DirectMethod.request({
                            url: '/Khazanedari/RegisterDocs/GetDefaultNum_Atf',
                            params: {
                                Year: data.fldYear
                            },
                            success: function (data) {
                                App.DocRegister_DetailsKhazane.queryById("txtDocNum").setValue(data.fldDocumentNum);
                                App.DocRegister_DetailsKhazane.queryById("txtAtfNum").setValue(data.fldAtfNum);
                                App.DocRegister_DetailsKhazane.queryById("txtDaily").setValue(data.ShomareRoozane);
                            }
                        });
                    }

                    App.DocRegister_DetailsKhazane.queryById("txtArchiveNum").setValue(data.fldArchiveNum);
                    App.DocRegister_DetailsKhazane.queryById("txtDescDoc").setValue(data.fldDescriptionDocu);
                    App.DocRegister_DetailsKhazane.queryById("txtDocDate").setRawValue(data.fldTarikhDocument);
                    SetValidateDocDate_Khazane();
                    App.DocRegister_DetailsKhazane.queryById("txtFareiNum").setValue(data.fldShomareFaree);                    
                    App.DocRegister_DetailsKhazane.queryById("CboDocumentType").setValue(data.fldTypeSanad);
                    App.DocRegister_DetailsKhazane.queryById("DocRegister_DetailsKhazaneGrid").store.load();
                    Ext.net.Mask.hide();
                }
            });
        }
        else {
            App.DocRegister_DetailsKhazane.queryById("txtDescDoc").validate();
            App.DocRegister_DetailsKhazane.queryById("txtDocDate").validate();
            App.DocRegister_DetailsKhazane.queryById("DocRegister_DetailsKhazaneGrid").store.load();
            App.DocRegister_DetailsKhazane.queryById("txtArchiveNum").validate();
        }
    }

    var loadCboHeadLine_Khazane = function (cellediting, opt) {
        if ('@ViewBag.HeaderId' == 0 || ('@ViewBag.HeaderId' != 0 && FirstLoadCboHeadLine_Khazane == 0)) {
