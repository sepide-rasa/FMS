    function sumColBes_Khazane() {
        var store = App.DocRegister_DetailsKhazane.queryById("DocRegister_DetailsKhazaneGrid").store;
        var summaryRec = App.DocRegister_DetailsKhazane.queryById("DocRegister_DetailsKhazaneGrid").getView().getFeature("summaryDocuments_Khazane").summaryRecord;
