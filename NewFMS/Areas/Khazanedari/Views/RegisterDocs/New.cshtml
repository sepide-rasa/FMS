            params: {
                Year: '@ViewBag.Year',
                Id: '@ViewBag.HeaderId'
            },
            success: function (data) {
                if ('@ViewBag.HeaderId' == "0") {
                    if (data.LastDate != "") {
                        App.DocRegister_DetailsKhazane.queryById("txtDocDate").setMinValue(new Date(data.LastDate));
                        App.DocRegister_DetailsKhazane.queryById("txtDocDate").minText = 'تاریخ باید پس از ' + data.LastDateShamsi + " باشد.";
                    }
                    else {/*هنوز رکوردی ثبت نشده*/
                        App.DocRegister_DetailsKhazane.queryById("txtDocDate").setMinValue(new Date(data.MinDate));
                        App.DocRegister_DetailsKhazane.queryById("txtDocDate").minText = 'تاریخ باید پس از ' + data.MinDateShamsi + " باشد.";
                    }
                    if ('@ViewBag.Year' == '@ViewBag.CurrentDate'.substring(0, 4)) {
                        App.DocRegister_DetailsKhazane.queryById("txtDocDate").setMaxValue('@ViewBag.CurrentDate');
                        App.DocRegister_DetailsKhazane.queryById("txtDocDate").maxText = 'تاریخ باید قبل از ' + '@ViewBag.CurrentDate' + " باشد.";
