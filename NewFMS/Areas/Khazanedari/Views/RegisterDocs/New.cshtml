                        App.DocRegister_DetailsKhazane.queryById("txtDocDate").maxText = 'تاریخ باید قبل از ' + data.MaxDateShamsi + " باشد.";
                    }
                }
                else {
                    App.DocRegister_DetailsKhazane.queryById("txtDocDate").setMaxValue('@ViewBag.CurrentDate');
