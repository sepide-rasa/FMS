    var addKeyMapDocRegister_DetailsKhazane = function (cmp) {
        this.keyMap = new Ext.util.KeyMap({
            target: cmp.getEl(),
            binding: [{
                key: [13, 9, 120], /*Ext.EventObject.ENTER,*/
                fn: function (key, e) {
                    if (e.getKey() == 9 || e.getKey() == 120) {
                        e.stopEvent();
                    }
                    if (e.getKey() == 120) {
                        SaveDocRegister_DetailsKhazane();
                    }
                    else {
                        var keyfield = App.DocRegister_DetailsKhazane.queryById(Ext.getCmp((Ext.Element.getActiveElement()).name).itemId);
                        var nextfld;
                        if (keyfield.itemId == "txtDescDoc") {
                            /*nextfld = App.ColHeadLine_Khazane.getComponent(0);*/
                            /*var grid = App.DocRegister_DetailsKhazane.queryById('DocRegister_DetailsKhazaneGrid');
                            grid.editingPlugin.startEdit(0, 2);*/
                            App.ColFile_Khazane.getComponent(0).focus();
                            return;
                        }
                        else if (keyfield.itemId == "txtFareiNum") {
                            if (App.DocRegister_DetailsKhazane.queryById("CboDocumentType").disabled == true) {
                                nextfld = App.DocRegister_DetailsKhazane.queryById("txtDocDate");
                            }
                            else {
                                nextfld = App.DocRegister_DetailsKhazane.queryById("CboDocumentType");
                            }
                        }
                        else {
                            nextfld = keyfield.nextSibling().nextSibling();
                            /*if (nextfld.itemId == "txtDescDoc") {
                                nextfld.el.swallowEvent(['keypress', 'keydown']);
                            }*/
                        }
                        nextfld.focus();
                        nextfld.selectText();
                    }
                }
            }]
        });
    };
