<script type="text/javascript">
    function CheckMablagh() {
        
         var txtAzMablagh = App.RptSodorChek.queryById('txtAzMablagh');
         var txtTaMablagh = App.RptSodorChek.queryById('txtTaMablagh');

         var mablagh1 = txtAzMablagh.getValue().replace(',', '').replace(',', '').replace(',', '').replace(',', '').replace(',', '');
         var mablagh2 = txtTaMablagh.getValue().replace(',', '').replace(',', '').replace(',', '').replace(',', '').replace(',', '');
         if (txtAzMablagh.getValue() != "") {
             if (parseFloat(mablagh2) < parseFloat(mablagh1)) {
                 Ext.MessageBox.show({
                     title: "خطا",
                     msg: "مبلغ وارد شده غیر مجاز است.",
                     icon: Ext.MessageBox.ERROR,
                     buttons: Ext.MessageBox.OK
                 });
                 txtTaMablagh.setValue("");
             }
         }
    }
    function PrintChek() {
        var SelectedRow = App.RptSodorChek.queryById('RptSodorChekGrid').selModel.getLastSelected();
        if (SelectedRow != null) {
            Ext.net.Mask.show({ msg: 'در حال بارگذاری...' });
            Ext.net.DirectMethod.request({
                url: '/Chek/SodorCheck/Print',
                params: {
                    SodorchekId: SelectedRow.data.fldId,
                    DasteChekId: SelectedRow.data.fldIdDasteCheck
                },
                success: function () {
                    Ext.net.Mask.hide();
                }
            });
        }
        else {
            Ext.MessageBox.show({
                title: "خطا",
                msg: "لطفا یک سطر را انتخاب کنید",
                icon: Ext.MessageBox.ERROR,
                buttons: Ext.MessageBox.OK
            });
        }
    }
    function ChangeAzTarikh() {
        var txtAzTarikh = App.RptSodorChek.queryById('txtAzTarikh');
        var txtTaTarikh = App.RptSodorChek.queryById('txtTaTarikh');
        txtAzTarikh.setMaxValue(txtTaTarikh.getValue());
        txtAzTarikh.maxText = "از تاریخ باید قبل از تا تاریخ " + txtTaTarikh.rawValue + "باشد";
    }
    function ChangeTaTarikh() {
        var txtAzTarikh = App.RptSodorChek.queryById('txtAzTarikh');
        var txtTaTarikh = App.RptSodorChek.queryById('txtTaTarikh');
        txtTaTarikh.setMinValue(txtAzTarikh.getValue());
        txtTaTarikh.minText = "تا تاریخ باید بعد از تاریخ " + txtAzTarikh.rawValue + "باشد";
    }

    function ChangeAzTarikh_V() {
