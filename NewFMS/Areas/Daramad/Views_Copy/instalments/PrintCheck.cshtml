@using Ext.Net.MVC;

@(
 Html.X().Window().Height(560)
    .Title("پیش نمایش")
    .ID("PrintrptBankforCheck")
    .AutoScroll(true)
    .Maximized(true)
    .Modal(true)
    .Listeners(l => l.AfterRender.Handler = "LoadReportBankforCheck();")
    .Closable(true)
    .CloseAction(Ext.Net.CloseAction.Destroy)
    .Html("<center><object class='se-pre-con' style='width: 100%;height: 100%;border: 1px solid #ccc;' type='application/pdf' data='/Daramad/instalments/GeneratePDFPrintCheck?OrganId=" + @ViewBag.OrganId + "&CheckId=" + @ViewBag.CheckId + "&ReplyTaghsitId=" + @ViewBag.ReplyTaghsitId + "'></object></center>")
)

<script>
    function LoadReportBankforCheck() {
        var a = $('object');
        var h = App.PrintrptBankforCheck.height;
        a.height(h-35);
    }
</script>