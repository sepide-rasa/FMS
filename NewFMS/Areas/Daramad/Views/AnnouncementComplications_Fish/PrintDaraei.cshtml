@using Ext.Net.MVC;

@(
 Html.X().Window()
    .ID("PrintDaraii")
    .Modal(true)
    .Width(925)
    .Height(500)
    .Title("پیش نمایش")
    .AutoScroll(true)
    .Closable(true)
    .Maximized(true)
    .CloseAction(Ext.Net.CloseAction.Destroy).Listeners(l => l.AfterRender.Handler = "LoadReport();")
        .Html("<center><object class='se-pre-con' style='width: 100%;height: 100%;border: 1px solid #ccc;' type='application/pdf' data='/AnnouncementComplications_Fish/GeneratePDFPrintDaraei?FishId=" + ViewBag.FishId + "&ElamAvarezId=" + ViewBag.ElamAvarezId + "'></object></center>")
)
<script>
    function LoadReport() {
        var a = $('object');
        var h = App.PrintDaraii.height;
        a.height(h-35);
    }
</script>

