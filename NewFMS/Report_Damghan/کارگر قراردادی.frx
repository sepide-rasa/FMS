    private void PageHeader1_AfterData(object sender, EventArgs e)
    {
      Text159.Text=&quot;تاریخ چاپ: &quot;+MyLib.Shamsi.Miladi2ShamsiString(((DateTime)Report.GetVariableValue(&quot;Date&quot;)))   ;
      Text160.Text=&quot;زمان چاپ: &quot;+((DateTime)Report.GetVariableValue(&quot;Date&quot;)).Hour+&quot;:&quot;+((DateTime)Report.GetVariableValue(&quot;Date&quot;)).Minute+&quot;:&quot;+((DateTime)Report.GetVariableValue(&quot;Date&quot;)).TimeOfDay.Seconds+&quot; &quot;;
    }

    
    int jamMablagh=0;
    private void Data4_AfterData(object sender, EventArgs e)
    {
      jamMablagh=jamMablagh+Convert.ToInt32(((Int32)Report.GetColumnValue(&quot;spr_tblHokm_ItemSelect.fldMablagh&quot;)));
      Cell61.Text=jamMablagh.ToString(&quot;#,###&quot;)+&quot; ریال&quot;;
      string k=MyLib.NumberTool.Num2Str(Convert.ToUInt64(jamMablagh),1)+&quot; ريال&quot;;                                         
      Cell65.Text=k;   
      string start,end;
      start=((String)Report.GetColumnValue(&quot;spr_tblPersonalHokmSelect.fldTarikhEjra&quot;)).ToString();
      end=((String)Report.GetColumnValue(&quot;spr_tblPersonalHokmSelect.fldTarikhEtmam&quot;)).ToString();
      int count=MyLib.Shamsi.DiffOfShamsiDate(start, end);

      Cell47.Text=toModat(count);
    }
    
    string toModat(int rozCount){
      int sal ,mah,roz;
      sal=Convert.ToInt32(rozCount/365);
      mah=Convert.ToInt32((rozCount-(sal*365))/30.41666666666667);
      roz=Convert.ToInt32((rozCount-(sal*365))-(mah*30.41666666666667));
      if(roz&lt;0)
        roz=0;
      return  &quot;2- مدت قرارداد: &quot;+sal.ToString()+&quot;سال و&quot;+mah.ToString()+&quot;ماه و&quot;+roz.ToString() +&quot;روز&quot;;
      
    }   
  }
}
