@{
    NewFMS.WCF_Common.CommonService servic = new NewFMS.WCF_Common.CommonService();
    NewFMS.WCF_Accounting.AccountingService Acc_servic = new NewFMS.WCF_Accounting.AccountingService();

    string IP = System.Web.HttpContext.Current.Request.ServerVariables["REMOTE_HOST"];
    NewFMS.WCF_Common.ClsError Err = new NewFMS.WCF_Common.ClsError();
    NewFMS.WCF_Accounting.ClsError Acc_Err = new NewFMS.WCF_Accounting.ClsError();
    string CurrYear = servic.GetDate(IP, out Err).fldTarikh.Substring(0, 4);
    bool HaveAccounting=false;
    bool HaveBudget=false;
    bool HaveKhazane=false;    

    var user = servic.GetUserWithFilter("fldUserName", Session["Username"].ToString(), "", 1, IP, out Err).FirstOrDefault();
    var LastLogin = servic.GetInputInfoWithFilter("fldUserId", user.fldId.ToString(), true, 2, IP, out Err).ToList();
    var FiscalYearId = 0;
    if (Session["OrganId"] != null)
    {
        var Row = Acc_servic.GetFiscalYearWithFilter("fldYear", CurrYear.ToString(), Convert.ToInt32(Session["OrganId"]), 1, IP, out Acc_Err).FirstOrDefault();
        if (Row != null)
        {
            FiscalYearId = Row.fldId;
        }
    }
    if (Session["OrganId"] != null)
    {
        var module_organ = servic.GetModule_OrganWithFilter("fldUserId", user.fldId.ToString(), 0, Session["Username"].ToString(), Session["Password"].ToString(),
        IP, out Err).ToList();

    HaveKhazane=module_organ.Where(l => l.fldModuleId == 10).Any();
    HaveAccounting = module_organ.Where(l => l.fldModuleId == 4).Any();
    HaveBudget = module_organ.Where(l => l.fldModuleId == 7).Any();
    }

    string _Date = "", Time = "", Ip = "", Name = "";
    int UsersId = 0;
    UsersId = user.fldId;
    int UserLoginCount = 0;
    UserLoginCount = NewFMS.Models.UserLoginCount.userObj.Count();

    Name = user.fldNameEmployee;
    if (LastLogin.Count == 2)
    {
        _Date = LastLogin[1].fldDateTime.ToString();
        Time = LastLogin[1].fldTime.ToString();
        Ip = LastLogin[1].fldIP.ToString();
    }

    var q = servic.GetOrganizationWithFilter("UserId", "", 0, Session["Username"].ToString(), (Session["Password"].ToString()), IP, out Err).ToList();
    var CountPosition = 0;
    CountPosition = q.Count();
}
